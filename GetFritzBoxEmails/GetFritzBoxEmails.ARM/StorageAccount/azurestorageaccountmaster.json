{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "deployment_release": {
      "defaultValue": "001",
      "type": "string"
    },
    "deployment_artifact_azureresources": {
      "defaultValue": "001",
      "type": "string"
    },
    "deployment_artifact_functionscode": {
      "defaultValue": "001",
      "type": "string"
    },
    "deployment_mode": {
      "defaultValue": "Incremental",
      "type": "string"
    },
    "deployment_location": {
      "defaultValue": "northeurope",
      "type": "string"
    },
    "storage_account_name": {
      "defaultValue": "storageaccountcwlabs01",
      "type": "string"
    }
  },
  "variables": {
  },
  "resources": [
    {
      "comments": "StorageAccount for AzureFunctions",
      "type": "Microsoft.Storage/storageAccounts",
      "sku": {
        "name": "Standard_LRS",
        "tier": "Standard"
      },
      "kind": "Storage",
      "name": "[parameters('storage_account_name')]",
      "apiVersion": "2018-07-01",
      "location": "[parameters('deployment_location')]",
      "tags": {
        "defaultExperience": "Core (SQL)",
        "ARM Template BuildId": "[parameters('deployment_artifact_azureresources')]",
        "Function BuildId": "[parameters('deployment_artifact_functionscode')]",
        "Release": "[parameters('deployment_release')]"
      },
      "scale": null,
      "properties": {
        "networkAcls": {
          "bypass": "AzureServices",
          "virtualNetworkRules": [],
          "ipRules": [],
          "defaultAction": "Allow"
        },
        "supportsHttpsTrafficOnly": false,
        "encryption": {
          "services": {
            "file": {
              "enabled": true
            },
            "blob": {
              "enabled": true
            }
          },
          "keySource": "Microsoft.Storage"
        }
      },
      "dependsOn": []
    }
  ],
  "outputs": {
  }
}
