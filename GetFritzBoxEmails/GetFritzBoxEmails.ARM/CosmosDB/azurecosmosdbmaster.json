{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "deployment_release": {
      "defaultValue": "001",
      "type": "string"
    },
    "deployment_artifact_azureresources": {
      "defaultValue": "001",
      "type": "string"
    },
    "deployment_artifact_functionscode": {
      "defaultValue": "001",
      "type": "string"
    },
    "deployment_mode": {
      "defaultValue": "Incremental",
      "type": "string"
    },
    "deployment_location": {
      "defaultValue": "northeurope",
      "type": "string"
    },
    "cosmosdb_account_name": {
      "defaultValue": "functionname",
      "type": "string"
    },
    "cosmosdb_account_default_experience": {
      "defaultValue": "Core (SQL)",
      "type": "string"
    }
  },
  "variables": {},
  "resources": [
    {
      "comments": "",
      "type": "Microsoft.DocumentDB/databaseAccounts",
      "kind": "GlobalDocumentDB",
      "name": "[parameters('cosmosdb_account_name')]",
      "apiVersion": "2015-04-08",
      "location": "[parameters('deployment_location')]",
      "tags": {
        "defaultExperience": "Core (SQL)"
      },
      "scale": null,
      "properties": {
        "ipRangeFilter": "",
        "enableAutomaticFailover": false,
        "enableMultipleWriteLocations": false,
        "isVirtualNetworkFilterEnabled": false,
        "virtualNetworkRules": [],
        "databaseAccountOfferType": "Standard",
        "consistencyPolicy": {
          "defaultConsistencyLevel": "Session",
          "maxIntervalInSeconds": 5,
          "maxStalenessPrefix": 100
        },
        "locations": [
          {
            "locationName": "[parameters('deployment_location')]",
            "provisioningState": "Succeeded",
            "failoverPriority": 0
          }
        ],
        "capabilities": []
      },
      "dependsOn": []
    }
  ]
}
